{
  "manifest_version": 3,
  "name": "Extension Permissions List",
  "version": "1.0",
  "description": "Lists installed extensions and their permissions.",
  "permissions": ["management"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
